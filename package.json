{
  "name": "leadlink-crm",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "echo \"Build command would run here\" && exit 0",
    "start": "next start",
    "lint": "next lint",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:staging": "npm run build && firebase deploy --only hosting:staging",
    "deploy:prod": "npm run build && firebase deploy --only hosting:production",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:import": "firebase emulators:import ./emulator-data",
    "prepare": "husky install",
    "test": "ts-node src/lib/testing/simpleTest.ts",
    "test:unit": "jest --testPathPattern=src/tests/unit",
    "test:integration": "ts-node src/lib/testing/testRunner.ts --filter integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:performance": "playwright test --project=chromium --config=playwright.perf.config.ts",
    "test:watch": "jest --watch",
    "test:ci": "ts-node src/lib/testing/simpleTest.ts",
    "test:auth": "ts-node src/lib/testing/testRunner.ts --filter auth",
    "test:firestore": "ts-node src/lib/testing/testRunner.ts --filter firestore",
    "validate:multitenant": "ts-node src/scripts/validateMultiTenant.ts",
    "test:visual": "playwright test --config=src/tests/visual/playwright.config.ts",
    "test:visual:update": "playwright test --config=src/tests/visual/playwright.config.ts --update-snapshots",
    "prebuild": "npm run test",
    "prestart": "tsc -p tsconfig.json",
    "test:all": "npm run test && npm run test:e2e"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
